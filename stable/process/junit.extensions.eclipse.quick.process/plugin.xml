<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.jdt.junit.testRunListeners">
      <testRunListener
            class="junit.extensions.eclipse.quick.process.internal.TestListenerForProcess">
      </testRunListener>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.eclipse.jdt.junit.preferences"
            class="junit.extensions.eclipse.quick.process.internal.preferences.ProcessPreferencePage"
            id="junit.extensions.eclipse.quick.process.Preference"
            name="Process Notification">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="junit.extensions.eclipse.quick.process.internal.preferences.PreferenceInitializer">
      </initializer>
   </extension>
</plugin>
